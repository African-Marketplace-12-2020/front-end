lState = {\n    data: []\n}\n\nexport default function (state = initialState, action) {\n    console.log(action.type)\n    switch(action.type) {\n    case ADD_USER: \n        return {\n            ...state, \n            profile: action.payload.user, \n            formSubmitted: false\n        }\n    case ADD_PRODUCT: \n        return {\n            ...state, \n            data: action.payload, \n            formSubmitted: false\n        }\n    case UPDATE_PRODUCT: \n        return {\n            ...state, \n            data: action.payload, \n            formSubmitted: false\n        }\n    case DELETE_PRODUCT: \n            return {\n                ...state, \n                data: state.data.data.filter(item => item.id !== action.payload)\n            }\n    case FETCH_START: \n        return {\n            ...state,\n            isFetching: true,\n            error: ''\n        }\n    case FETCH_SUCCESS: \n        return {\n            ...state, \n            isFetching: false,\n            data: action.payload,\n            error: null\n        }\n    case FETCH_FAIL:\n        return {\n            ...state,\n            error: action.payload\n        }\n        default: \n            return state\n    }\n}","/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/Login.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/actions/marketActions.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/reducers/index.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/Signup.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/App.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/Navbar.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/Categories.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/ProductForm.