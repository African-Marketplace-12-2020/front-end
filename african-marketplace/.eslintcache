ken');\n    //props.history.push('/login')\n    }\n\n\n  return (\n    <Container>\n      <Hamburguer onClick={() => toggleDisplay()}>\n        <Line />\n        <Line />\n        <Line />\n      </Hamburguer>\n      <NavLinks display={display}>\n        <li style={{ color: \"white\", textDecoration: \"none\", fontSize: \"16px\" }}>\n          <NavLink to=\"/login\">Login</NavLink>\n        </li>\n        <li style={{ color: \"white\", textDecoration: \"none\", fontSize: \"16px\" }}>\n            <NavLink to=\"/login\" onClick={logout}>Logout</NavLink>\n        </li>\n        <li style={{ color: \"white\", textDecoration: \"none\", fontSize: \"16px\" }}>\n            <NavLink to=\"/product-list\">Product List</NavLink>\n        </li>\n      </NavLinks>\n    </Container>\n  );\n}\n",{"ruleId":"8","severity":1,"message":"9","line":2,"column":19,"nodeType":"10","messageId":"11","endLine":2,"endColumn":27},"no-unused-vars","'Redirect' is defined but never used.","Identifier","unusedVar"]