owrap;\n      }\n`;\n\nconst ProductList = (props) => {\n    const [editing, setEditing] = useState();\n    useEffect(() => {\n        props.fetchData();\n    }, [])\n\n    return (\n        <div>\n        <Container>\n            <ProductForm />\n            <FlexContainer>\n\n            {props.isFetching ? (\n                    <CircleLoader />\n                ) : ( \n                    props.productsAsProps && props.productsAsProps.map(item => \n                        <div key={item.id}>\n                            <ItemCard data={item} editProduct={editing} />\n                        </div>\n                )\n            )}\n            </FlexContainer>\n        </Container>\n        </div>\n    )\n}\n\nconst mapStateToProps = (state) => {\n    //console.log(state)\n    return {\n        productsAsProps: state.marketReducer.data,\n        isFetching: state.marketReducer.isFetching,\n        error: state.marketReducer.error\n    }\n  }\n\nexport default connect(mapStateToProps, {fetchData, deleteProduct})(ProductList)","/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/ProductForm.js",["74","75"],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/EditProduct.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/ItemCard.js",["76","77"],"import React, { useState } from \"react\";\nimport styled from \"styled-components\";\nimport { connect } from 'react-redux';\nimport { updateProduct, deleteProduct } from '../actions/marketActions';\nimport EditProduct from './forms/EditProduct';\n\n\nconst Container = styled.div`\n  border: black solid 2px;\n  background-color: #ffefd5;\n  width: 325px;\n  border-radius: 10px;\n  border: #77925f solid 1px;\n  margin: 10px 5px;\n  padding: 5px;\n  @media screen and (max-width: 768px) {\n    width: 300px;\n  }\n`;\n\nconst Title = styled.h1`\n  color: #77925f;\n  font-weight: normal;\n`;\n\nconst Text = styled.p`\n  color: #77925f;\n  font-weight: normal;\n  margin: 10px 0px;\n`;\n\nconst initialItemToEdit = {\n  name: \"\",\n  pantone_value: \"\",\n  year: \"\",\n};\n\nconst ItemCard = (props) => {\n  const [editing, setEditing] = useState(false);\n  const [itemToEdit, setItemToEdit] = useState(initialItemToEdit);\n\n  const editItem = item => {\n    setEditing(!editing);\n  };\n\n  const cancelEdit = item => {\n    setEditing(false);\n  };\n\n  return (\n    <div>\n    <Container>\n      <Title>{props.data.name}</Title>\n      <div\n        style={{\n          display: \"flex\",\n          flexDirection: \"column\",\n          textAlign: \"left\",\n          marginLeft: \"5px\",\n        }}>\n        <Text>Description: {props.data.description}</Text>\n        <Text>Price: ${props.data.price}.00</Text>\n        <Text>Location: {props.data.location}</Text>\n        <Text>Category: {props.data.category}</Text>\n        \n      </div>\n      {!editing && (\n          <div className=\"button-row\">\n              <button onClick={() => editItem()}>Edit Item</button>\n              \n              <button onClick={() => {props.deleteProduct(props.data.id)}}>Delete</button>\n          </div>\n      )}\n        {editing && (\n            <EditProduct data={props.data} editItem={editItem} editing={editing} />\n        )}\n        {editing && (\n            <button onClick={() => cancelEdit(props.data)}>Cancel</button>\n        )}\n    </Container>\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  return {\n      productsAsProps: state.marketReducer.data.data,\n      isFetching: state.marketReducer.isFetching,\n      error: state.marketReducer.error\n  }\n}\n\nexport default connect(mapStateToProps,{updateProduct, deleteProduct})(ItemCard);\n","/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/index.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/Login.js",[],"/Library/WebServer/Documents/Lamda/front-end/afri