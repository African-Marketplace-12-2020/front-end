 from \"styled-components\";\n\nconst Container = styled.div`\n  height: 10vh;\n  margin: 0.5rem;\n  padding: 1rem;\n  background: #fff;\n  @media screen and (max-width: 768px) {\n    position: relative;\n  }\n`;\n\nconst FlexContainer = styled.div`\n    width: 100%;\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n    @media screen and (max-width: 768px) {\n        flex-direction: column;\n        flex-wrap: nowrap;\n      }\n`;\n\nconst CircleLoaderContainer = styled.div`\n      margin: 0 auto;\n`;\n\nconst ProductList = (props) => {\n    const [editing, setEditing] = useState();\n    useEffect(() => {\n        props.fetchData();\n    }, [])\n\n    return (\n        <div>\n        <Container>\n            <ProductForm />\n            <FlexContainer>\n\n            {props.isFetching ? (\n                    <CircleLoader />\n                ) : ( \n                    props.productsAsProps && props.productsAsProps.map(item => \n                        <div key={item.id}>\n                            <ItemCard data={item} editProduct={editing} />\n                        </div>\n                )\n            )}\n            </FlexContainer>\n        </Container>\n        </div>\n    )\n}\n\nconst mapStateToProps = (state) => {\n    //console.log(state)\n    return {\n        productsAsProps: state.marketReducer.data,\n        isFetching: state.marketReducer.isFetching,\n        error: state.marketReducer.error\n    }\n  }\n\nexport default connect(mapStateToProps, {fetchData, deleteProduct})(ProductList)","/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/ProductForm.js",["57","58"],"import React, { useState, useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport { addProduct, updateProduct } from '../../actions/marketActions';\nimport styled from \"styled-components\";\n\nconst Container = styled.nav`\n  height: 10vh;\n  width: 100%;\n  margin: 0.5rem;\n  padding: 1rem;\n  background: #fff;\n  @media screen and (max-width: 768px) {\n    position: relative;\n  }\n`;\n\nexport const FormGroup = styled.form`\n\tcolor: black;\n    display: block;\n\t  width: 40%;\n    margin: 50px auto;\n    padding: 2rem;\n    -moz-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n    -webkit-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n    box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n    @media screen and (max-width: 768px) {\n      width: 60%;\n      margin: 30px auto;\n    }\n`;\n\nexport const Input = styled.input`\n\tpadding: 0.5em;\n\tcolor: palevioletred;\n\tbackground: papayawhip;\n\tborder: none;\n\tborder-radius: 3px;\n\twidth: 98%;\n    margin-bottom: 0.5em;\n`;\n\nconst ProductForm = (props) => {\n    //console.log(props)\n    const [values, setValues] = useState({\n      name: '',\n      description: '',\n      price: ''\n    });\n\n    useEffect(() => {\n        props.editProduct && setValues(props.editProduct);\n    }, [props.updateProduct])\n\n    const handleProductChange = (e) => {\n      setValues({ ...values, [e.target.name]: e.target.value });\n    };\n\n    const handleSubmit = (e) => {\n      e.preventDefault();\n      console.log(values)\n      props.addProduct(values);\n      setValues({\n        name: '',\n        description: '',\n        price: ''\n      })\n    };\n\n    return (\n        <div>\n          <FormGroup onSubmit={handleSubmit}>\n            <h1>Product Form</h1>\n            <Input id=\"label\" \n                placeholder='Name' \n                type=\"text\"\n                name=\"name\"\n                value={values.name || ''}\n                onChange={handleProductChange}\n            />\n            <Input  \n                placeholder='Description' \n                type=\"text\"\n                name=\"description\"\n                value={values.description || ''}\n                onChange={handleProductChange}\n            />\n            <Input \n                placeholder='Price' \n                type=\"text\"\n                name=\"price\"\n                value={values.price || ''}\n                onChange={handleProductChange}\n            />\n\n\n          <div className=\"form-group\">\n                <label>Location:    </label>\n                <select\n                    className=\"form-control\" \n                    onChange={e =>\n                      setValues({ ...values, location: e.target.value })\n                    }\n                    value={values.location}\n                >\n                  <option value=\"\">--Please choose a location--</option>\n                  <option value=\"Bungoma\">Bungoma</option>\n                  <option value=\"Isiolo\">Isiolo</option>\n                  <option value=\"Nairobi\">Nairobi</option>\n                </select>\n              </div>\n\n              <div className=\"form-group\">\n                <label>Category:    </label>\n                <select\n                    className=\"form-control\" \n                    onChange={e =>\n                      setValues({ ...values, category: e.target.value })\n                    }\n                    value={values.category}\n                >\n                  <option value=\"\">--Please choose a Category--</option>\n                  <option value=\"1\">Clothing & Apparel</option>\n                  <option value=\"2\">Authentic Artwork</option>\n                  <option value=\"3\">Food Items</option>\n                </select>\n              </div>\n            <button>Submit</button>\n            </FormGroup>\n        </div>\n    )\n}\n\nexport default connect(\n  () => {\n    return {}\n}, {addProduct, updateProduct})(ProductForm)","/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/EditProduct.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/ItemCard.js",["59","60"],"import React, { useState } from \"react\";\nimport styled from \"styled-components\";\nimport { connect } from 'react-redux';\nimport { updateProduct, deleteProduct } from '../actions/marketActions';\nimport EditProduct from './forms/EditProduct';\n\n\nconst Container = styled.div`\n  border: black solid 2px;\n  background-color: #ffefd5;\n  width: 325px;\n  border-radius: 10px;\n  border: #77925f solid 1px;\n  margin: 10px 5px;\n  padding: 5px;\n  @media screen and (max-width: 768px) {\n    width: 300px;\n  }\n`;\n\nconst Title = styled.h1`\n  color: #77925f;\n  font-weight: normal;\n`;\n\nconst Text = styled.p`\n  color: #77925f;\n  font-weight: normal;\n  margin: 10px 0px;\n`;\n\nconst initialItemToEdit = {\n  name: \"\",\n  pantone_value: \"\",\n  year: \"\",\n};\n\nconst ItemCard = (props) => {\n  //console.log(props)\n  const [editing, setEditing] = useState(false);\n  const [itemToEdit, setItemToEdit] = useState(initialItemToEdit);\n\n  const editItem = item => {\n    setEditing(!editing);\n  };\n\n  const cancelEdit = item => {\n    setEditing(false);\n  };\n\n  return (\n    <div>\n    <Container>\n      <Title>{props.data.name}</Title>\n      <div\n        style={{\n          display: \"flex\",\n          flexDirection: \"column\",\n          textAlign: \"left\",\n          marginLeft: \"5px\",\n        }}>\n        <Text>Description: {props.data.description}</Text>\n        <Text>Price: ${props.data.price}</Text>\n        <Text>Location: {props.data.location}</Text>\n        <Text>Category: {props.data.category}</Text>\n        \n      </div>\n      {!editing && (\n          <div className=\"button-row\">\n              <button onClick={() => editItem()}>Edit Item</button>\n              \n              <button onClick={() => {props.deleteProduct(props.data.id)}}>Delete</button>\n          </div>\n      )}\n        {editing && (\n            <EditProduct data={props.data} editItem={editItem} editing={editing} />\n        )}\n        {editing && (\n            <button onClick={() => cancelEdit(props.data)}>Cancel</button>\n        )}\n    </Container>\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  return {\n      productsAsProps: state.marketReducer.data.data,\n      isFetching: state.marketReducer.isFetching,\n      error: state.marketReducer.error\n  }\n}\n\nexport default connect(mapStateToProps,{updateProduct, deleteProduct})(ItemCard);\n","/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/index.js",[],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/components/forms/Login.js",["61"],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/reducers/authReducer.js",["62"],"/Library/WebServer/Documents/Lamda/front-end/african-marketplace/src/actions/marketActions.js",["63"],{"ruleId":"64","severity":1,"message":"65","line":5,"column":8,"nodeType":"66","messageId":"67","endLine":5,"endColumn":14},{"ruleId":"64","severity":1,"message":"68","line":10,"column":8,"nodeType":"66","messageId":"67","endLine":10,"endColumn":23},{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"64","severity":1,"message":"73","line":30,"column":7,"nodeType":"66","messageId":"67","endLine":30,"endColumn":28},{"ruleId":"64","severity":1,"message":"74","line":35,"column":21,"nodeType":"66","messageId":"67","endLine":35,"endColumn":31},{"ruleId":"75","severity":1,"message":"76","line":38,"column":8,"nodeType":"77","endLine":38,"endColumn":10,"suggestions":"78"},{"ruleId":"64","severity":1,"message":"79","line":6,"column":7,"nodeType":"66","messageId":"67","endLine":6,"endColumn":16},{"ruleId":"75","severity":1,"message":"80","line":52,"column":8,"nodeType":"77","endLine":52,"endColumn":29,"suggestions":"81"},{"ruleId":"64","severity":1,"message":"82","line":41,"column":10,"nodeType":"66","messageId":"67","endLine":41,"endColumn":20},{"ruleId":"64","severity":1,"message":"83","line":41,"column":22,"nodeType":"66","messageId":"67","endLine":41,"endColumn":35},{"ruleId":"64","severity":1,"message":"84","line":3,"column":10,"nodeType":"66","messageId":"67",